<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>servelet1</servlet-name>
        <servlet-class>servelet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servelet2</servlet-name>
        <servlet-class>servelet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servelet3</servlet-name>
        <servlet-class>servelet3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servelet4</servlet-name>
        <servlet-class>servelet4</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servelet1</servlet-name>
        <url-pattern>/servelet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servelet2</servlet-name>
        <url-pattern>/servelet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servelet3</servlet-name>
        <url-pattern>/servelet3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servelet4</servlet-name>
        <url-pattern>/servelet4</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
